{
  "name": "sharingbuttons.io",
  "version": "1.1.0",
  "description": "Super fast and easy Social Media Sharing Buttons. No JavaScript. No tracking.",
  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.0.0",
    "react": "^0.12.0",
    "jquery": "^2.1.4",
    "react-inlinesvg": "0.4.1"
  },
  "devDependencies": {
    "browserify": "~6.2.0",
    "envify": "~3.0.0",
    "reactify": "^0.15",
    "watchify": "~2.1.0",
    "local-web-server": "",
    "uglify-js": ""
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d . | ws",
    "build": "browserify . | uglifyjs -cm > js/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mstoiber/sharing.git"
  },
  "author": "Max Stoiber",
